<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.campus_share.mapper.PostLikeMapper">
    <select id="selectLikeUserIdsByPostId" resultType="java.lang.Long">
        SELECT user_id
        FROM t_post_like
        WHERE post_id = #{postId}
    </select>
</mapper> 